class Factorial{
    public static void main(String[] a){
		f = new Fac();

	System.out.println(f.ComputeFac(10));
	System.out.println(f.length);
    }
}

class Fac {

    public int ComputeFac(int num){
	int num_aux ;
	if (num < 1)
	    num_aux = 1 ;
	else 
	    num_aux = num * (this.ComputeFac(num-1)) ;
	return num_aux ;
    }

}

